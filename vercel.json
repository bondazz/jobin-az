{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/storage/v1/object/public/sitemaps/sitemap.xml"
    },
    {
      "source": "/sitemaps.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/storage/v1/object/public/sitemaps/sitemaps.xml"
    },
    {
      "source": "/sitemap_new.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/storage/v1/object/public/sitemaps/sitemap_new.xml"
    },
    {
      "source": "/sitemap_job.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/sitemap-jobs"
    },
    {
      "source": "/sitemap1.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/sitemap-xml"
    },
    {
      "source": "/sitemap_index.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/serve-sitemap?file=sitemap_index.xml"
    },
    {
      "source": "/sitemap_main.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/serve-sitemap?file=sitemap_main.xml"
    },
    {
      "source": "/sitemapjooble.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/serve-sitemap?file=sitemapjooble.xml"
    },
    {
      "source": "/sitemap-static.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/serve-sitemap?file=sitemap-static.xml"
    },
    {
      "source": "/sitemap-categories.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/serve-sitemap?file=sitemap-categories.xml"
    },
    {
      "source": "/sitemap_static.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/serve-sitemap?file=sitemap_static.xml"
    },
    {
      "source": "/sitemap_categories.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/serve-sitemap?file=sitemap_categories.xml"
    },
    {
      "source": "/sitemap_companies.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/serve-sitemap?file=sitemap_companies.xml"
    },
    {
      "source": "/sitemap_jobs.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/serve-sitemap?file=sitemap_jobs.xml"
    },
    {
      "source": "/sitemap-jobs-:page.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/serve-sitemap?file=sitemap-jobs-:page.xml"
    },
    {
      "source": "/sitemap-companies-:page.xml",
      "destination": "https://igrtzfvphltnoiwedbtz.supabase.co/functions/v1/serve-sitemap?file=sitemap-companies-:page.xml"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    }
  ]
}
